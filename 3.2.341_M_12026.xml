  <Step stepId="i1"> 
	 <StepTitle> 
		<StepNumber>1</StepNumber> <Text>SAFING</Text>
</StepTitle> 
	 <ClarifyingInfo infoType="warning" isNumbered="false" itemId="i2"> 
		<InfoText>
<Para>
<Text>Surrounding equipment may be hot. Use caution
when working inside rack.</Text>
</Para> 
		</InfoText> 
	 </ClarifyingInfo> 

	 <Step stepId="i3">
<StepTitle>
<StepNumber>1.1</StepNumber>
<Instruction>
<VerifyCallout verifyType="check">
<CenterName>MCC-H</CenterName>
<Text> that the following have been completed:</Text>
</VerifyCallout>
</Instruction>
</StepTitle> 
		<StepContent itemId="i4" spacingAbove="single">
<Instruction>
<ClearText>
<Text/>
<Symbol name="nbsp"/>
<Symbol name="nbsp"/>
<Symbol name="nbsp"/>
<Text>RPCM LAD62B_A PRC 04 Op, Cl cmd Inh</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i5" spacingAbove="single">
<Instruction>
<ClearText>
<Text/>
<Symbol name="nbsp"/>
<Symbol name="nbsp"/>
<Symbol name="nbsp"/>
<Text>MCA deactivated and cooled for 30 minutes</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<ClarifyingInfo infoType="note" isNumbered="false" itemId="i6"> 
		  <InfoText>
<Para>
<Text>All lines, connectors removed from MCA, AR Rack during
access to MCA must be secured out of the way to avoid
damage during rack rotation and MCA Drawer translation.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 
  </Step> 

  <Step stepId="i150">
<StepTitle>
<StepNumber>2</StepNumber>
<Text>RACK ROTATION DOWN</Text>
</StepTitle> 

	 <Step stepId="i151">
<StepTitle>
<StepNumber>2.1</StepNumber>
<Instruction>
<VerifyCallout verifyType="check">
<Text>Rack rotation path unobstructed</Text>
</VerifyCallout>
</Instruction>
</StepTitle> 
		<StepContent itemId="i152" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Disengage rack K-Bar thumb latches (two) from Standoff.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i153" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Slowly rotate rack down to a controlled stop.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i154" spacingAbove="single">
<Instruction>
<VerifyCallout verifyType="verify">
<Text>ITCS Sampling Tool will not be impacted.</Text>
</VerifyCallout>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i155" spacingAbove="single">
<Instruction>
<ClearText>
<Text>As required, temporarily restrain rack [Adjustable Tethers].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 
  </Step> 

  <Step stepId="i156">
<StepTitle>
<StepNumber>3</StepNumber>
<Text>DISCONNECTING MCA REAR UTILITIES</Text>
</StepTitle> 

	 <Step stepId="i12">
<StepTitle>
<StepNumber>3.1</StepNumber>
<Instruction>
<ClearText>
<Text>Unfasten rear access panel fasteners (eight) [Ratchet, 1/4" Drive; 4" Ext; 5/32" Hex Head].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i13" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Temporarily stow.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i14"> 
		  <Image display="inline">
<ImageReference alt="3.2.341_M_12026_files/12026f1.JPG" height="444" source="3.2.341_M_12026_files/12026f1.JPG" width="824"/> 
			 <ImageTitle>
<ImageNumber>1</ImageNumber>
<Text>Rear View of MCA.</Text> 
			 </ImageTitle> 
		  </Image> 
		</StepContent> 
		<ClarifyingInfo infoType="caution" isNumbered="false" itemId="i15"> 
		  <InfoText>
<Para>
<Text>P-Clamp is noncaptive. Expect a spacer and nut to
be freed when P-Clamp is released.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i16">
<StepTitle>
<StepNumber>3.2</StepNumber>
<Instruction>
<ClearText>
<Text>Remove AAA hose noncaptive P-Clamp mounting bolt from rack interface [Ratchet, 1/4" Drive; 5/16" Socket; 1/4" Combination Wrench].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i17" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Place P-Clamp, nut, spacer in Ziplock Bag.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i18" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Temporarily stow.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i19" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd1.pdf">
<Text>Figure 1</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i20"> 
		  <Image display="inline">
<ImageReference alt="3.2.341_M_12026_files/12026f2.JPG" height="230" source="3.2.341_M_12026_files/12026f2.JPG" width="819"/> 
			 <ImageTitle>
<ImageNumber>2</ImageNumber>
<Text>Sample Line Block, View from Rear of MCA.</Text> 
			 </ImageTitle> 
		  </Image> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i21">
<StepTitle>
<StepNumber>3.3</StepNumber>
<Instruction>
<ClearText>
<Text>Maneuver AAA hose to gain access to sample line block mounting
bolt.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i22" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd1.pdf">
<Text>Figure 1</Text>
</Hyperlink>
</ReferenceInfo>
<Text>) (</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd2.pdf">
<Text>Figure 2</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<ClarifyingInfo infoType="caution" isNumbered="false" itemId="i23"> 
		  <InfoText>
<Para>
<Text>Sample line block contains eight spring-loaded ports. Spring
force will be released upon disengaging mounting bolt.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i24">
<StepTitle>
<StepNumber>3.4</StepNumber>
<Instruction>
<ClearText>
<Text>Disconnect sample line block from Sample Distribution Assembly, one mounting bolt [Ratchet, 1/4" Drive; 5/32" Hex Head].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i25" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Cover sample line block with Ziplock Bag, secure with Kapton Tape.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i26" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd1.pdf">
<Text>Figure 1</Text>
</Hyperlink>
</ReferenceInfo>
<Text>) (</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd2.pdf">
<Text>Figure 2</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i27">
<StepTitle>
<StepNumber>3.5</StepNumber>
<Instruction>
<ClearText>
<Text>Don Static Wrist Tether.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i28" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Secure to unpainted, unanodized metal surface, except stowage racks.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i29">
<StepTitle>
<StepNumber>3.6</StepNumber>
<Text>Demating Connectors</Text>
</StepTitle> 
		<StepContent itemId="i30" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd1.pdf">
<Text>Figure 1</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i31" spacingAbove="single">
<Instruction>
<ClearText>
<Text>W9-P5 </Text>
<Symbol name="odf-disconnect-symbol"/>
<Text> J1</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i32" spacingAbove="single">
<Instruction>
<ClearText>
<Text>W55-P6 </Text>
<Symbol name="odf-disconnect-symbol"/>
<Text> J2</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i33">
<StepTitle>
<StepNumber>3.7</StepNumber>
<Instruction>
<ClearText>
<Text>Cover connectors, jacks (two each) with Kapton Tape.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i34" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Tether connectors to rack, not to MCA, to prevent obstruction when
sliding MCA out of rack (Velcro Straps).</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i35">
<StepTitle>
<StepNumber>3.8</StepNumber>
<Instruction>
<ClearText>
<Text>Doff Static Wrist Tether.</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 
  </Step> 

  <Step stepId="i157">
<StepTitle>
<StepNumber>4</StepNumber>
<Text>RACK ROTATION UP</Text>
</StepTitle> 

	 <Step stepId="i36">
<StepTitle>
<StepNumber>4.1</StepNumber>
<Instruction>
<VerifyCallout verifyType="check">
<Text>Rack rotation path unobstructed</Text>
</VerifyCallout>
</Instruction>
</StepTitle> 
		<StepContent itemId="i37" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Rack K-Bar thumb latches (two) </Text>
<Symbol name="odf-right-arrow"/>
<Text> Up</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i38" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Slowly rotate rack up to a controlled stop.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i39">
<StepTitle>
<StepNumber>4.2</StepNumber>
<Instruction>
<ClearText>
<Text>Engage rack K-Bar thumb latches (two) into Standoff Capture
Mechanism.</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 
  </Step> 

  <Step stepId="i158">
<StepTitle>
<StepNumber>5</StepNumber>
<Text>DISCONNECTING MCA DRAWER UTILITIES</Text>
</StepTitle> 

	 <Step stepId="i40">
<StepTitle>
<StepNumber>5.1</StepNumber>
<Instruction>
<ClearText>
<Text>Unlatch AR Rack lower left Rack Access Door latches (two).</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i41" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Open lower left AR Rack Access Door.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i42"> 
		  <Image display="inline">
<ImageReference alt="3.2.341_M_12026_files/12026f3.JPG" height="408" source="3.2.341_M_12026_files/12026f3.JPG" width="934"/> 
			 <ImageTitle>
<ImageNumber>3</ImageNumber>
<Text>MCA (View from Bottom): Vacuum Line and Avionics Air Line Location.</Text> 
			 </ImageTitle> 
		  </Image> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i43">
<StepTitle>
<StepNumber>5.2</StepNumber>
<Instruction>
<VerifyCallout verifyType="check">
<TargetObject>
<Text>Manual Valve (HV01)</Text>
</TargetObject>
<VerifyOperator operator="longDash"/>
<TargetValue>
<Text>CLOSED</Text>
</TargetValue>
</VerifyCallout>
</Instruction>
</StepTitle> 
		<ClarifyingInfo infoType="note" isNumbered="false" itemId="i44"> 
		  <InfoText>
<Para>
<Text>There is no access to place a Ziplock Bag over exposed
vacuum line while MCA Drawer remains in the rack. The Ziplock Bag will be placed over MCA Vacuum Line
after MCA Drawer is pulled out.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i45">
<StepTitle>
<StepNumber>5.3</StepNumber>
<Instruction>
<ClearText>
<Text>Disconnect vacuum line beneath MCA [1" Combination Wrench].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i46" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd3.pdf">
<Text>Figure 3</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<ClarifyingInfo infoType="note" isNumbered="true" itemId="i47"> 
		  <InfoText>
<Para>
<Text>Avionics Air Assembly (AAA) hose flange clamp bolt, nut (noncaptive) must be removed from clamp for clamp
removal.</Text>
</Para> 
		  </InfoText> 
		  <InfoText>
<Para>
<Text>Be aware that AAA will remain operational during MCA
Maintenance activity. AAA is required for Trace
Contaminant Control Subsystem (TCCS) operation.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i48">
<StepTitle>
<StepNumber>5.4</StepNumber>
<Instruction>
<ClearText>
<Text>Remove AAA hose flange clamp [Ratchet, 1/4" Drive; 5/16" Socket; Driver Handle; 3/8" Deep Socket].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i49" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Stow clamp bolt, nut in Ziplock Bag, temporarily stow.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i50" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd3.pdf">
<Text>Figure 3</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i159">
<StepTitle>
<StepNumber>5.5</StepNumber>
<Instruction>
<ClearText>
<Text>Disconnect AAA hose from MCA (expect air flow into hose).</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i160" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Cover exposed AAA hose with Ziplock Bag, secure with Gray Tape
to prevent FOD.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 
  </Step> 

  <Step stepId="i161">
<StepTitle>
<StepNumber>6</StepNumber>
<Text>ACCESSING MCA</Text>
</StepTitle> 

	 <Step stepId="i51"> 
		<StepContent itemId="i53"> 
		  <Image display="inline">
<ImageReference alt="3.2.341_M_12026_files/12026f4.JPG" height="509" source="3.2.341_M_12026_files/12026f4.JPG" width="951"/> 
			 <ImageTitle>
<ImageNumber>4</ImageNumber>
<Text>MCA Drawer Pulled Out of AR Rack.</Text> 
			 </ImageTitle> 
		  </Image> 
		</StepContent> 
		<ClarifyingInfo infoType="warning" isNumbered="false" itemId="i54"> 
		  <InfoText>
<Para>
<Text>Verification Gas Assembly contains pressurized
gas. Failure to close HV02 Verification Gas Supply
may result in injury to crew or equipment damage.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i55">
<StepTitle>
<StepNumber>6.1</StepNumber>
<Instruction>
<ClearText>
<Text>Turn HV02 Verification Gas Supply to CLOSED (</Text>
<Symbol name="odf-clockwise-sign"/>
<Text>) until torque
reading of 28 in-lbs [(5-35 in-lbs) Trq Driver, 1/4" Drive; 1/4" to 3/8" Adapter].</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i56">
<StepTitle>
<StepNumber>6.2</StepNumber>
<Instruction>
<ClearText>
<Text>Attach 8.5" Handrail to front of MCA.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<ClarifyingInfo infoType="note" isNumbered="true" itemId="i57"> 
		  <InfoText>
<Para>
<Text>Captive fasteners disengage after approximately six to
eight </Text>
<Symbol name="odf-counterclockwise-sign"/>
<Text> turns when unfastened.</Text>
</Para> 
		  </InfoText> 
		  <InfoText>
<Para>
<Text>Expect MCA Drawer to be stiff when sliding out of rack.</Text>
</Para> 
		  </InfoText> 
		  <InfoText>
<Para>
<Text>Top right MCA Drawer fastener has a loose nut plate
and is only hand tight.</Text>
</Para> 
		  </InfoText> 
		  <InfoText>
<Para>
<Text>Bottom left door on AR Rack must remain open in order
to slide MCA Drawer out.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i58">
<StepTitle>
<StepNumber>6.3</StepNumber>
<Instruction>
<ClearText>
<Text>Loosen top right MCA Drawer fastener by hand.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i59" spacingAbove="single">
<Instruction>
<ClearText>
<Text>If nut plate spins, open upper left Rack Access Door to secure nut
plate by hand while loosening fastener.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i60">
<StepTitle>
<StepNumber>6.4</StepNumber>
<Instruction>
<ClearText>
<Text>Unfasten MCA Drawer fasteners (three) one full </Text>
<Symbol name="odf-counterclockwise-sign"/>
<Text> turn
at a time in an X pattern until each fastener disengages [Ratchet, 1/4" Drive; 4" Ext; 9/64" Hex Head].</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i61">
<StepTitle>
<StepNumber>6.5</StepNumber>
<Instruction>
<ClearText>
<Text>Slide MCA Drawer out of AR Rack until hard stop and lock.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i62" spacingAbove="single">
<Instruction>
<VerifyCallout verifyType="verify">
<Text>slide locks (one each side) engaged.</Text>
</VerifyCallout>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i63" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Place Ziplock Bag over exposed vacuum line, secure with Gray
Tape.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i64" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd3.pdf">
<Text>Figure 3</Text>
</Hyperlink>
</ReferenceInfo>
<Text>) (</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd4.pdf">
<Text>Figure 4</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 
  </Step> 

  <Step stepId="i65">
<StepTitle>
<StepNumber>7</StepNumber>
<Text>ACCESSING MCA VERIF GAS ASSY</Text>
</StepTitle> 
	 <StepContent itemId="i66"> 
		<Image display="inline">
<ImageReference alt="3.2.341_M_12026_files/12026f5.JPG" height="566" source="3.2.341_M_12026_files/12026f5.JPG" width="492"/> 
		  <ImageTitle>
<ImageNumber>5</ImageNumber>
<Text>Top View of MCA Drawer.</Text> 
		  </ImageTitle> 
		</Image> 
	 </StepContent> 
	 <StepContent itemId="i67"> 
		<Image display="inline">
<ImageReference alt="3.2.341_M_12026_files/12026f6.JPG" height="346" source="3.2.341_M_12026_files/12026f6.JPG" width="729"/> 
		  <ImageTitle>
<ImageNumber>6</ImageNumber>
<Text>Illustration of MCA Verification Gas Assembly.</Text> 
		  </ImageTitle> 
		</Image> 
	 </StepContent> 

	 <Step stepId="i68">
<StepTitle>
<StepNumber>7.1</StepNumber>
<Instruction>
<ClearText>
<Text>On MCA Verif Gas Assy, push button, rotate lifting handle (one) into upright locked position.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i69" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd5.pdf">
<Text>Figure 5</Text>
</Hyperlink>
</ReferenceInfo>
<Text>) (</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd6.pdf">
<Text>Figure 6</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i70">
<StepTitle>
<StepNumber>7.2</StepNumber>
<Instruction>
<ClearText>
<Text>Unfasten MCA Verif Gas Assy jacking fasteners (six) one full </Text>
<Symbol name="odf-counterclockwise-sign"/>
<Text> turn at a time in an X pattern until each fastener disengages [Ratchet, 1/4" Drive; 10" Ext; 3/16" Hex Head].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i71" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd5.pdf">
<Text>Figure 5</Text>
</Hyperlink>
</ReferenceInfo>
<Text>) (</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd6.pdf">
<Text>Figure 6</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<ClarifyingInfo infoType="caution" isNumbered="false" itemId="i72"> 
		  <InfoText>
<Para>
<Text>To avoid damage to rubber gaskets on pneumatic
ports, exercise care during stowage of removed
MCA Verif Gas Assy.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i73">
<StepTitle>
<StepNumber>7.3</StepNumber>
<Instruction>
<ClearText>
<Text>Remove MCA Verif Gas Assy from MCA Drawer by handle.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i74" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Inspect black rubber gaskets (five) on pneumatic ports for visible
damage or debris.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i75" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Inspect electrical connector for bent pins or debris (some pin
locations may be empty).</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i76" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Push button, rotate lifting handle down into stowed position.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i77" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Label MCA Verif Gas Assy "Failed" [Gray Tape; Sharpie].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i78" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Temporarily stow.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 
  </Step> 

  <Step stepId="i79">
<StepTitle>
<StepNumber>8</StepNumber>
<Text>MCA CHASSIS/MCA VERIF GAS ASSY INSPECTION</Text>
</StepTitle> 
	 <ClarifyingInfo infoType="caution" isNumbered="false" itemId="i80"> 
		<InfoText>
<Para>
<Text>MCA Chassis connector socket insulating material is easily
damaged by connector misalignment and may cause FOD
to enter socket.</Text>
</Para> 
		</InfoText> 
	 </ClarifyingInfo> 

	 <Step stepId="i81">
<StepTitle>
<StepNumber>8.1</StepNumber>
<Instruction>
<ClearText>
<Text>Inspect connector interfaces on chassis for visible damage or debris.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i82" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Clean chassis as required [Dry Wipes].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i83" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Photo document [Digital Camera].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i84">
<StepTitle>
<StepNumber>8.2</StepNumber>
<Instruction>
<ClearText>
<Text>Remove protective plugs, caps, tape from pneumatic ports and
electrical connector on replacement MCA Verif Gas Assy.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i85" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Place plugs, caps from replacement ORU on removed MCA Verif
Gas Assy.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i86" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Temporarily stow removed MCA Verif Gas Assy.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i87">
<StepTitle>
<StepNumber>8.3</StepNumber>
<Instruction>
<ClearText>
<Text>Inspect replacement MCA Verif Gas Assy connector for bent pins or
debris (some pin locations may be empty) and inspect rubber gaskets (five) on pneumatic ports for visible damage or debris.</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 
  </Step> 

  <Step stepId="i88">
<StepTitle>
<StepNumber>9</StepNumber>
<Text>REPLACING VERIF GAS ASSY</Text>
</StepTitle> 

	 <Step stepId="i89">
<StepTitle>
<StepNumber>9.1</StepNumber>
<Instruction>
<ClearText>
<Text>Push button, rotate lifting handle (one) into upright position.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i90" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Install replacement Verif Gas Assy in MCA Drawer by placing
assembly on guide pins in drawer.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i91" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Push button, rotate lifting handle down into stowed position.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i92">
<StepTitle>
<StepNumber>9.2</StepNumber>
<Instruction>
<ClearText>
<Text>Firmly push down Verif Gas Assy with slight rocking action (&lt; 1/2 inch rocking motion, grasping top edge of Verif Gas Assy) to engage connector.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<ClarifyingInfo infoType="caution" isNumbered="false" itemId="i93"> 
		  <InfoText>
<Para>
<Text>Due to Blind Mate Connector engagement, avoid using jacking
fasteners to force connector mate. Jacking fasteners should
engage with moderate torque until five to six </Text>
<Symbol name="odf-clockwise-sign"/>
<Text> turns.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
		<ClarifyingInfo infoType="note" isNumbered="false" itemId="i94"> 
		  <InfoText>
<Para>
<Text>MCA Verif Gas Assy has five pneumatic fittings mated in compression. Slower torquing or pushing sequence is required.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i95">
<StepTitle>
<StepNumber>9.3</StepNumber>
<Instruction>
<ClearText>
<Text>Fasten jacking fasteners (six) one full </Text>
<Symbol name="odf-clockwise-sign"/>
<Text> turn in an X pattern [Ratchet, 1/4" Drive; 10" Ext; 3/16" Hex Head].</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i96">
<StepTitle>
<StepNumber>9.4</StepNumber>
<Instruction>
<ClearText>
<Text/>
<GotoStep startStep="i92">
<PreliminaryInstructions>
<Text>Repeat</Text>
</PreliminaryInstructions>
<DepartureDescription>
<Text>step 9.2</Text>
</DepartureDescription>
</GotoStep>
<Text> and </Text>
<GotoStep startStep="i95">
<DepartureDescription>
<Text>step 9.3</Text>
</DepartureDescription>
</GotoStep>
<Text> until noticeable increase in torque (five to six </Text>
<Symbol name="odf-clockwise-sign"/>
<Text> turns total on each fastener).</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i97">
<StepTitle>
<StepNumber>9.5</StepNumber>
<Instruction>
<ClearText>
<Text>Torque jacking fasteners (six) to 64 in-lbs [(40-200 in-lbs) Trq Wrench, 1/4" Drive; 10" Ext; 3/16" Hex Head].</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 
  </Step> 

  <Step stepId="i98">
<StepTitle>
<StepNumber>10</StepNumber>
<Text>CLOSING OUT MCA</Text>
</StepTitle> 
	 <ClarifyingInfo infoType="note" isNumbered="false" itemId="i99"> 
		<InfoText>
<Para>
<Text>After installation of the new Verification Gas Assembly, </Text>
<CenterName>MCC-H</CenterName>
<Text> should verify ops products have been updated to reflect the new
calibration gas coefficients.</Text>
</Para> 
		</InfoText> 
	 </ClarifyingInfo> 

	 <Step stepId="i100">
<StepTitle>
<StepNumber>10.1</StepNumber>
<Instruction>
<ClearText>
<Text>Photo document MCA Drawer [DCS 760 Camera].</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i101">
<StepTitle>
<StepNumber>10.2</StepNumber>
<Instruction>
<ClearText>
<Text>Remove Ziplock Bag from MCA Vacuum Line.</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i102">
<StepTitle>
<StepNumber>10.3</StepNumber>
<Instruction>
<ClearText>
<Text>Release MCA slide locks (one each side) by pressing in slide lock on
both sides of drawer.</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i103">
<StepTitle>
<StepNumber>10.4</StepNumber>
<Instruction>
<ClearText>
<Text>Slide MCA Drawer to closed position.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<ClarifyingInfo infoType="note" isNumbered="false" itemId="i104"> 
		  <InfoText>
<Para>
<Text>Top right MCA Drawer fastener has a loose nut plate. This
fastener will not be reengaged.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i105">
<StepTitle>
<StepNumber>10.5</StepNumber>
<Instruction>
<ClearText>
<Text>Tighten, torque MCA Drawer fasteners (three of four) to 28 in-lbs [Ratchet, 1/4" Drive; 4" Ext; 9/64" Hex Head; (5-35 in-lbs) Trq Driver].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i106" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd4.pdf">
<Text>Figure 4</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i107">
<StepTitle>
<StepNumber>10.6</StepNumber>
<Instruction>
<ClearText>
<Text>Remove 8.5" Handrail.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i108" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Temporarily stow.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 
  </Step> 

  <Step stepId="i162">
<StepTitle>
<StepNumber>11</StepNumber>
<Text>CONNECTING MCA DRAWER UTILITIES</Text>
</StepTitle> 

	 <Step stepId="i109">
<StepTitle>
<StepNumber>11.1</StepNumber>
<Instruction>
<ClearText>
<Text>Remove Ziplock Bag from AAA hose.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i110" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Connect AAA hose to MCA Assembly.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i111" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Fasten AAA hose flange clamp, torque to 13 in-lbs [Ratchet, 1/4" Drive; 5/16" Socket; (5-35 in-lbs) Trq Driver; 3/8" Deep Socket].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i112" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd3.pdf">
<Text>Figure 3</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i113">
<StepTitle>
<StepNumber>11.2</StepNumber>
<Instruction>
<ClearText>
<Text>Connect MCA Vacuum Line beneath MCA hand tight.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i114" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Torque vacuum line to 6 in-lbs [(0-30 in-lbs) Dial Trq Wrench, 1/4" Drive; 1/4" to 3/8" Adapter; 1" Crowfoot, 3/8" Drive].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i115" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd3.pdf">
<Text>Figure 3</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i116">
<StepTitle>
<StepNumber>11.3</StepNumber>
<Instruction>
<ClearText>
<Text>Photo document MCA Drawer Utilities [Digital Camera].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<ClarifyingInfo infoType="note" isNumbered="false" itemId="i117"> 
		  <InfoText>
<Para>
<Text>From previous crew comments, one of the hinges on
lower left door was slightly bent and may cause difficulty
in getting it to stay closed.</Text>
</Para> 
		  </InfoText> 
		</ClarifyingInfo> 
	 </Step> 

	 <Step stepId="i118">
<StepTitle>
<StepNumber>11.4</StepNumber>
<Instruction>
<ClearText>
<Text>Close AR Rack lower left Rack Access Door.</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 
  </Step> 

  <Step stepId="i163">
<StepTitle>
<StepNumber>12</StepNumber>
<Text>RACK ROTATION DOWN</Text>
</StepTitle> 

	 <Step stepId="i119">
<StepTitle>
<StepNumber>12.1</StepNumber>
<Instruction>
<VerifyCallout verifyType="check">
<Text>Rack rotation path unobstructed</Text>
</VerifyCallout>
</Instruction>
</StepTitle> 
		<StepContent itemId="i120" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Disengage rack K-Bar thumb latches (two) from Standoff.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i121" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Slowly rotate rack down to a controlled stop.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i122" spacingAbove="single">
<Instruction>
<ClearText>
<Text>As required, temporarily restrain rack [Adjustable Tethers].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 
  </Step> 

  <Step stepId="i164">
<StepTitle>
<StepNumber>13</StepNumber>
<Text>CONNECTING MCA REAR UTILITIES</Text>
</StepTitle> 
	 <ClarifyingInfo infoType="note" isNumbered="false" itemId="i123"> 
		<InfoText>
<Para>
<Text>Sample Line Block requires compression force while
engaging mounting bolt due to eight spring-loaded ports.</Text>
</Para> 
		</InfoText> 
	 </ClarifyingInfo> 

	 <Step stepId="i124">
<StepTitle>
<StepNumber>13.1</StepNumber>
<Instruction>
<ClearText>
<Text>Install Sample Line Block on back of MCA, maintain positive
pressure on Sample Line Block (by hand) torque to 70 in-lbs [Ratchet, 1/4" Drive; 5/32" Hex Head; (40-200 in-lbs) Trq Wrench].</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i125" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd2.pdf">
<Text>Figure 2</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i126">
<StepTitle>
<StepNumber>13.2</StepNumber>
<Instruction>
<ClearText>
<Text>Maneuver AAA hose back into position with P-Clamp mounting bolt.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i127" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Tighten P-Clamp mounting bolt into rack, torque to 28 in-lbs [Ratchet, 1/4" Drive; 5/16" Socket; (5-35 in-lbs) Trq Driver; 1/4" Combination Wrench].</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i128" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd1.pdf">
<Text>Figure 1</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i129">
<StepTitle>
<StepNumber>13.3</StepNumber>
<Instruction>
<ClearText>
<Text>Don Static Wrist Tether.</Text>
</ClearText>
</Instruction>
</StepTitle> 
		<StepContent itemId="i130" spacingAbove="single">
<Instruction>
<ClearText>
<Text>Secure to unpainted, unanodized metal surface, except stowage racks.</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i131">
<StepTitle>
<StepNumber>13.4</StepNumber>
<Instruction>
<ClearText>
<Text>Untether and remove covers from connectors, jacks (two each).</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i132">
<StepTitle>
<StepNumber>13.5</StepNumber>
<Text>Mating Connectors</Text>
</StepTitle> 
		<StepContent itemId="i133" spacingAbove="single">
<Instruction>
<ClearText>
<Text>W9-P5 </Text>
<Symbol name="odf-connect-symbol"/>
<Text> J1</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i134" spacingAbove="single">
<Instruction>
<ClearText>
<Text>W55-P6 </Text>
<Symbol name="odf-connect-symbol"/>
<Text> J2</Text>
</ClearText>
</Instruction> 
		</StepContent> 
		<StepContent itemId="i135" spacingAbove="single">
<Instruction>
<ClearText>
<Text>(</Text>
<ReferenceInfo>
<Hyperlink target="RefDocs/SODF/IFM/12026/12026rd1.pdf">
<Text>Figure 1</Text>
</Hyperlink>
</ReferenceInfo>
<Text>)</Text>
</ClearText>
</Instruction> 
		</StepContent> 
	 </Step> 

	 <Step stepId="i136">
<StepTitle>
<StepNumber>13.6</StepNumber>
<Instruction>
<ClearText>
<Text>Doff Static Wrist Tether.</Text>
</ClearText>
</Instruction>
</StepTitle> 
	 </Step> 

	 <Step stepId="i137">
